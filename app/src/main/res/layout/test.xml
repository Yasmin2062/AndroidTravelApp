<?xml version="1.0"?>

<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <ScrollView
        android:id="@+id/credit_card_checkout_____checkout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent">


        <RelativeLayout
            android:id="@+id/content_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <RelativeLayout
                android:id="@+id/page_credit_card_checkout_____checkout_ek1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp">

                <View
                    android:id="@+id/_bg__credit_card_checkout_____checkout_ek2"
                    android:layout_width="390dp"
                    android:layout_height="844dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/_bg__credit_card_checkout_____checkout_ek2_shape" />

                <View
                    android:id="@+id/unsplash_bq7nhy4trga"
                    android:layout_width="390dp"
                    android:layout_height="844dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/unsplash_bq7nhy4trga_shape" />


                <RelativeLayout
                    android:id="@+id/group_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="88dp">


                    <RelativeLayout
                        android:id="@+id/master_card_3__simple_purple_"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="130dp">

                        <View
                            android:id="@+id/card_surface"
                            android:layout_width="342dp"
                            android:layout_height="190dp"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:background="@drawable/card_surface_shape" />

                        <View
                            android:id="@+id/gousian_layer"
                            android:layout_width="342dp"
                            android:layout_height="190dp"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:alpha="0.029999999329448"
                            android:background="@drawable/gousian_layer_shape" />

                        <ImageView
                            android:id="@+id/effect"
                            android:layout_width="288.16dp"
                            android:layout_height="134.31dp"
                            android:layout_marginStart="53.34dp"
                            android:layout_marginTop="55.17dp"
                            android:contentDescription="effect"
                            android:src="@drawable/effect" />

                        <TextView
                            android:id="@+id/number"
                            android:layout_width="254dp"
                            android:layout_height="36dp"
                            android:layout_marginLeft="24.52dp"
                            android:layout_marginTop="83dp"
                            android:fontFamily="Montserrat"
                            android:text="@string/number_string"
                            android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                            android:textSize="16sp"
                            android:textStyle="bold" />


                        <RelativeLayout
                            android:id="@+id/group_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="25dp"
                            android:layout_marginTop="119dp">

                            <TextView
                                android:id="@+id/put_name_here"
                                android:layout_width="142dp"
                                android:layout_height="36dp"
                                android:layout_marginLeft="0dp"
                                android:layout_marginTop="20dp"
                                android:fontFamily="Montserrat"
                                android:text="@string/put_name_here_string"
                                android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/put_name_here_ek1"
                                android:layout_width="68dp"
                                android:layout_height="36dp"
                                android:layout_marginLeft="197dp"
                                android:layout_marginTop="20dp"
                                android:fontFamily="Montserrat"
                                android:text="@string/put_name_here_ek1_string"
                                android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/credit_text"
                                android:layout_width="82dp"
                                android:layout_height="33dp"
                                android:layout_marginLeft="197dp"
                                android:layout_marginTop="0dp"
                                android:alpha="0.9"
                                android:fontFamily="Montserrat"
                                android:text="@string/credit_text_string"
                                android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                                android:textSize="12sp" />

                            <TextView
                                android:id="@+id/credit_text_ek1"
                                android:layout_width="55dp"
                                android:layout_height="33dp"
                                android:layout_marginLeft="0dp"
                                android:layout_marginTop="0dp"
                                android:alpha="0.9"
                                android:fontFamily="Montserrat"
                                android:text="@string/credit_text_ek1_string"
                                android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                                android:textSize="12sp" />

                        </RelativeLayout>


                        <RelativeLayout
                            android:id="@+id/logo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="25dp"
                            android:layout_marginTop="25dp">

                            <View
                                android:id="@+id/ellipse_1"
                                android:layout_width="30.65dp"
                                android:layout_height="30.65dp"
                                android:layout_marginStart="0dp"
                                android:layout_marginTop="9.13dp"
                                android:background="@drawable/ellipse_1_shape" />

                            <View
                                android:id="@+id/ellipse_2"
                                android:layout_width="30.65dp"
                                android:layout_height="30.65dp"
                                android:layout_marginStart="18.39dp"
                                android:layout_marginTop="9.13dp"
                                android:background="@drawable/ellipse_2_shape" />

                            <ImageView
                                android:id="@+id/intersect"
                                android:layout_width="14.15dp"
                                android:layout_height="24.52dp"
                                android:layout_marginStart="17.44dp"
                                android:layout_marginTop="12.2dp"
                                android:contentDescription="intersect"
                                android:src="@drawable/intersect" />

                        </RelativeLayout>

                    </RelativeLayout>

                    <TextView
                        android:id="@+id/textHome"
                        android:layout_width="161dp"
                        android:layout_height="54dp"
                        android:layout_gravity="center_horizontal|center"
                        android:layout_marginLeft="95dp"
                        android:layout_marginTop="0dp"
                        android:fontFamily="Montserrat"
                        android:gravity="center"
                        android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                        android:textSize="28sp"
                        android:textStyle="bold" />


                    <RelativeLayout
                        android:id="@+id/group_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="98dp">

                        <TextView
                            android:id="@+id/saved_cards"
                            android:layout_width="105dp"
                            android:layout_height="27dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="3dp"
                            android:fontFamily="Montserrat"
                            android:text="@string/saved_cards_string"
                            android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                            android:textSize="14sp" />


                        <RelativeLayout
                            android:id="@+id/group_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="219dp"
                            android:layout_marginTop="0dp">

                            <TextView
                                android:id="@+id/add_new"
                                android:layout_width="77dp"
                                android:layout_height="27dp"
                                android:layout_gravity="center_vertical|end"
                                android:layout_marginLeft="24dp"
                                android:layout_marginTop="3dp"
                                android:fontFamily="Montserrat"
                                android:gravity="end"
                                android:text="@string/add_new_string"
                                android:textColor="@color/add_new_color"
                                android:textSize="14sp" />


                            <RelativeLayout
                                android:id="@+id/plus"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_marginTop="0dp">

                                <ImageView
                                    android:id="@+id/vector"
                                    android:layout_width="0dp"
                                    android:layout_height="14dp"
                                    android:layout_marginStart="12dp"
                                    android:layout_marginTop="5dp"
                                    android:contentDescription="vector"
                                    android:src="@drawable/vector" />

                                <ImageView
                                    android:id="@+id/vector_ek1"
                                    android:layout_width="14dp"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="5dp"
                                    android:layout_marginTop="12dp"
                                    android:contentDescription="vector_ek1"
                                    android:src="@drawable/vector_ek1" />

                            </RelativeLayout>

                        </RelativeLayout>

                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@+id/group_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="73dp"
                        android:layout_marginTop="606dp">

                        <View
                            android:id="@+id/rectangle_1"
                            android:layout_width="196dp"
                            android:layout_height="50dp"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:background="@drawable/rectangle_1_shape" />

                        <TextView
                            android:id="@+id/proceed"
                            android:layout_width="111dp"
                            android:layout_height="38dp"
                            android:layout_gravity="center_horizontal|center"
                            android:layout_marginLeft="47dp"
                            android:layout_marginTop="12dp"
                            android:fontFamily="Montserrat"
                            android:gravity="center"
                            android:text="@string/proceed_string"
                            android:textColor="@color/_bg__credit_card_checkout_____checkout_ek2_color"
                            android:textSize="18sp"
                            android:textStyle="bold" />

                    </RelativeLayout>

                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

    </ScrollView>

</HorizontalScrollView>